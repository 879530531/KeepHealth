package com.gxuwz.KeepHealth.business.entity;

/**
 * 听读器音频文件管理表
 * @author 叶城廷
 * @version 2019.02.01
 */
public class TbReadDevice  implements java.io.Serializable {


    // Fields    

     private Integer readId;//听读器ID
     private Integer readNumber;//听读器数字
     private String readFilePath;//听读器音频文件路径


    // Constructors

    /** default constructor */
    public TbReadDevice() {
    }

    
    /** full constructor */
    public TbReadDevice(Integer readNumber, String readFilePath) {
        this.readNumber = readNumber;
        this.readFilePath = readFilePath;
    }

   
    // Property accessors

    public Integer getReadId() {
        return this.readId;
    }
    
    public void setReadId(Integer readId) {
        this.readId = readId;
    }

    public Integer getReadNumber() {
        return this.readNumber;
    }
    
    public void setReadNumber(Integer readNumber) {
        this.readNumber = readNumber;
    }

    public String getReadFilePath() {
        return this.readFilePath;
    }
    
    public void setReadFilePath(String readFilePath) {
        this.readFilePath = readFilePath;
    }
   








}