@charset "utf-8";

* {
	margin: 0;
	padding: 0;
}

  body,p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td,
	form, fieldset, legend, input, textarea, button, select {
	margin: 0;
	padding: 0;
}

 body, input, textarea, select, button, table {
	font-size: 16px;
	line-height: 1.25em;
}

 body {
	min-width: 320px;
	font-size: 1em;
	height:auto;
	font-family: PingFangSC-Regular,Helvetica,"Droid Sans",Arial,sans-serif;
	color: #000;
	-webkit-text-size-adjust: none;
	background: #fff;
} 

img, fieldset {
	border: 0;
}

ul, ol {
	list-style: none;
}

em, address {
	font-style: normal;
}

a {
	text-decoration: none;
	outline: 0 none;
}

table {
	border-collapse: collapse;
}
.clearfix:after { /* 清除浮动 */
content: ".";
display: block;
height: 0;
clear: both;
visibility: hidden;
}
.title {
	margin: 0 50px;
    text-align: center;
    height: 44px;
    line-height: 44px;
    font-size: 16px;
}

.header {
	font-family: "PingHei", "Lucida Grande", "Lucida Sans Unicode",
		"STHeiti", "Helvetica", "Arial", "Verdana", "sans-serif";
	color: #252525;
	text-decoration: none;
	font-size: 16px;
}

.header-bar{
	position: relative;
    background: #efefef;
    background-size: 100% 44px;
    min-height: 44px;
    border-bottom: 1px solid #bfbfbf;
    border-width: 0 0 1px 0;
    border-image: url() 2 0 round;
}

.icon-back{
	position: absolute;
    width: 40px;
    height: 44px;
}

.icon-back span{
	width: 20px;
    height: 20px;
    background-position: -20px 0;
    margin: 12px 0 0 10px;
	text-indent: -100px;
    overflow: hidden;
	display: block;
	background-image:url(../images/back.png)
}

.header-menu{
	position: absolute;
    right: 0;
    top: 0;
    width: 40px;
    height: 44px;
    overflow: hidden;
}

.header-menu span{
	width: 20px;
	height: 20px;
	background-position: -60px 0;
	margin: 12px 10px 0 10px;
	text-indent: -100px;
	overflow: hidden;
	display: block;
	background-image: url(../images/header-menu.png);
}
.header-menu .header-edit{
display:block;margin-top:12px;color: black;	font-size: 16px;
}

.down-menu{
    width: 100%;
    background: #404042;
    height: 58px;
    list-style-type: none;
}

.down-menu li{
    display: table-cell;
	float:left;
    width: 25%;
    text-align: center;
	list-style: none;
}

.down-menu a{
    border: 0;
    color: #fff;
}

.down-menu span{
	display: block;
	width: 30px;
	height: 30px;
	color: #fff;
	font-weight: normal;
	font-size: 12px;
	margin-top: 6px;
	margin-right: auto;
	margin-left: auto;
}
/*主页图标 size:30X30*/
.down-menu span.home{
	background-image:url(../images/df-home.png);
}
/*网上缴费图标 size:30X30*/
.down-menu span.free{
	background-image:url(../images/df-free.png);
}
/*搜索图标 size:30X30*/
.down-menu span.categoties{
	background-image:url(../images/df-search.png);
}
/*个人中心费图标 size:30X30*/
.down-menu span.center{
	background-image:url(../images/df-usr.png);
}

.down-menu strong{
	display: block;
    padding-bottom: 2px;
    margin: 0 auto;
    color: #fff;
    font-weight: normal;
    font-size: 12px;
}

.down-menu li.current{
    background: #2d2d2d;
}

.footer{
    background: #f8f8f8;
    color: #848689;
    text-align: center;
    font-size: 12px;
	font-family: "PingHei","Lucida Grande", "Lucida Sans Unicode", "STHeiti", "Helvetica","Arial","Verdana","sans-serif";
}

.footer-menu{
    width: 320px;
    margin: 0 auto;
    display: table;
    height: 13px;
    padding: 18px 0;
}

.footer-menu li{
    display: table-cell;
    width: 69px;
    height: 13px;
    line-height: 13px;
    font-size: 13px;
	list-style-type: none;
	border-right: 1px solid #d7d7d7;
}

.footer-menu li:last-child {
    border:0;
}

.footer-menu a{
    color: #848689;
    text-decoration: none;
	display: inline-block;
    width: 72px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 1.1em;
}

.seperator{
	height: 0px;
    border-top: 1px solid #bfbfbf;
    border-width: 1px 0 0 0;
}

.footer-platforms{
    height: 69px;
    width: 320px;
    margin: 0 auto;
    display: table;
}

.footer-platforms a{
	float:left;
    display: block;
    width: 106px;
    margin: 0 auto;
    color: #848689;
    text-decoration: none;
    position: relative;
}
.footer-platforms li.current a {
    color: #252525;
}
.footer-platforms li a:before {
    content: "";
    display: block;
    height: 38px;
    width: 33px;
    margin: 9px auto 0;
    opacity: 0.6;
}
  /*app版背景图标 size:33x38*/
.footer-platforms li.app a:before{
	background-image: url(../images/app-platforms.png);
}
  /*手机版背景图标 size:33x38*/
.footer-platforms li.touch a:before{
	background-image: url(../images/touch_platforms.png);
}
  /*电脑版背景图标 size:33x38*/
.footer-platforms li.computer a:before{
	background-image: url(../images/computer-platforms.png);
}
.copyright{
    height: 44px;
    line-height: 44px;
    text-align: center;
    color: #848689;
    font-size: 12px;
}
.common-header{
	min-height: 45px;
	max-width: 640px;
	min-width: 320px;
    margin-top: 0px;
    margin: 0px auto;
}
.common_bottom{
	min-height:170px;
	max-width: 640px;
	min-width: 320px;
	margin: 0px auto;
}
.common-container{
	width: 100%;
	position: relative;
	max-width: 100%;
	min-width: 320px;
	overflow: hidden;
	margin:0 auto;
}
.container-head{
    width: 100%;
	height:100px;
    color: #FFF;
    position: relative;
}
.container-head .user-bg{
    height: 100px;
    width: 100%;
    position: absolute;
    overflow: hidden;
}
.container-head .user-info{
	margin: 0 15px 7px 4px;
    padding-top: 7px;
    z-index: 20;
    font-size: 0;
    position: relative;
}
.avatar{
    width: 76px;
    height: 80px;
    position: relative;
    vertical-align: middle;
    margin-right: 4px;
}
.my-img{
    width: 56px;
    height: 56px;
    border-radius: 50%;
    overflow: hidden;
    position: absolute;
    top: 12px;
    left: 10px;
    z-index: 1;
}
.my-img img{
	position:absolute;
    width: 56px;
    height: 56px;
}
.container-head .person-info{
	position:absolute;
	top:3px;
	left:100px;
    height: 55px;
}
.container-head .head-name{
    font-size: 17px;
    line-height: 17px;
    height: 17px;
    margin-top: 16px;
    margin-bottom: 22px;
}
.container-head .head-member{
	position:absolute;
	top:60px;
	left:100px;
}
.container-head .head-type{
    font-size: 13px;
    line-height: 13px;
    margin-right: 10px;
}
.party-member{
    font-size: 12px;
    height: 20px;
	width:65px;
    line-height: 20px;
    border: 1px solid rgba(76,74,72,0.5);
    border-radius: 10px;
    background-color: rgba(76,74,72,0.3);
	position:absolute;
	top:-4px;
	left:65px;
}
.member-text{
    margin-left: 8px;
    margin-right: 8px;
}
.container-head .head-message{
	position: absolute;
    right: 5px;;
    top: 37px;
}
.message-name{
    font-size: 12px;
    color: #e3e5e9;
    line-height: 12px;
    margin-right: 10px;
}
.icon-hint{
	position:absolute;
	top:1px;
	right:0px;
	display:block;
    width: 5px;
    height: 10px;
	background-image:url(../images/right.png);
    background-size: 5px 10px;
}
.my-content{
    margin-top: 10px;
}
.bdr-bottom{
	position:relative;
}
.bdr-bottom:after{
    height: 1px;
    content: '';
    width: 100%;
    border-top: 1px solid #dadada;
    position: absolute;
    bottom: -1px;
    right: 0;
    z-index: 10;
}
.list-item .bdr-bottom:after{
    height: 1px;
    content: '';
    width: 100%;
    border-top: 1px solid #dadada;
    position: absolute;
    bottom: 0px;
    right: 0;
    z-index: 10;
}
.bdr-top{
	position:relative;
}
.bdr-top:before{
	height: 1px;
    content: '';
    width: 100%;
    border-bottom: 1px solid #dadada;
    position: absolute;
    top: -1px;
    left: 0;
    z-index: 10;
}
.content-banner{
	display:block;
	width:100%;
    height: 44px;
    line-height: 44px;
    background: #fff;
    padding: 0 10px;
}
.item-left{
    float: left;
}
.item-right{
    float:right;
}
.w-20{
    width: 20px;
	margin-right:5px;
}
.item-icon{
	position:relative;
	top:-3px;
	display: inline-block;
	height: 100%;
}
.item-icon img{
	width:20px;
    vertical-align: middle;
}
.item-left-text{
	position:relative;
	top:-2px;
	display:inline-block;
	font-size: 14px;
    color: #232326;
}
.item-left-text2{
	position:relative;
	top:-2px;
	display:inline-block;
	font-size: 14px;
    color: #878787;
}
.item-right .text-info{
	display:block;
	position:absolute;
	top:0px;
	right:0px;
    font-size: 12px;
    color: #848689;
    margin-right: 30px;
}
.icon-right{
	display:block;
	position:absolute;
	top:5px;
	right:12px;
	width: 9px;
    height: 16px;
    margin-top: 8px;
	background-image:url(../images/right.png);
	background-size:9px 16px;
}
.item-bottom{
    padding: 17px 0 6px 0;
    background: #fff;
    display: -webkit-box;/*内容平均分配*/
}
.item-bottom a{
	display: block;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    width: 1%;
}
.part-ID{
    text-align: center;
    position: relative;
    font-size: 0;
}
.ID-img{
    width: 20px;
    height: 20px;
    margin: 0 auto;
    margin-bottom: 4px;
    position: relative;
    display: block;
}
.ID-img img{
    width: 100%;
    height: 100%;
	border: 0;
    vertical-align: middle;
}
.ID-text{
	display: block;
    font-size: 11px;
    line-height: 11px;
    color: #232326;
    text-align: center;
    margin-bottom: 4px;
}
.ID-num{
    display: block;
    height: 15px;
    margin: 0 auto;
    margin-bottom: 12px;
    font-size: 15px;
    color: #232326;
}
.content-list{
    width: 100%;
    margin-top: 10px;
    background-color: white;
    overflow: hidden;
}
.content-header-list{
    width: 100%;
    background-color: white;
    overflow: hidden;
}
.list-item{
    height: 44px;
    line-height: 44px;
    padding: 0 0 0 10px;
    width: 100%;
    overflow: hidden;
    display: -webkit-box;
    display: -moz-box;
    vertical-align: middle;
}
.list-item-info{
    vertical-align: middle;
    padding-right: 20px;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
	position:relative;
}
.list-item-info .icon-right{
	display:block;
	margin-right:10px;
}
.list-text-info{
    display:block;
	position:absolute;
	top:0px;
	right:0px;
    font-size: 12px;
    color: #848689;
    margin-right: 39px;
}
.list-text-info2 {
    display:block;
	position:absolute;
	top:-1px;
	left:30%;
    font-size: 14px;
    color: #000000;
    margin-right: 39px;
}
.my-form{
	position: relative;
    padding: 0;
    border: 0;
    background-color: #fff;
}
.t-header{
	font-size:12px;padding:5px 10px;background-color: rgb(233,233,233);color:#999;
}
.t-label{
	float: left; width: 30%; color:#878787;
}
.t-input{
	border:none;outline: none;
}
.main-nav{
	position: relative;
    width: 100%;
    top: 0;
}
.section{
    font-size: 0.5em;
    padding-left: 0.15rem;
    padding-right: 0.1rem;
    padding-bottom: 0.5rem;
    background-color:#fff;
}
.my-article-img{
	float: left;    width: 30%;overflow: hidden;position: relative;margin-right: 0.4rem;
}
.my-article-img img{
	width: 100%;    display: block;
}
.my-article-info{
	color:#404040;
}
.my-article-title{
	margin-bottom:0.2em;margin-top:0.2em;text-align:left;line-height: 0.83rem;font-size: 0.78rem;height:1.9rem;overflow: hidden;text-overflow:ellipsis;
}
.desc-l{
	float: left;
}
.desc-r {
	float: right;
    background-size: contain;
    background-position: left 0;
    background-repeat: no-repeat;
    color: #888;
    font-size: 0.6rem;
    line-height: 1.0rem;
}
.my-article-channel{
    display: inline-block;
    color: #f33;
    font-size: 0.5rem;
    padding-top: 2px;
    padding-right: 3px;
    padding-bottom: 2px;
    padding-left: 3px;
    line-height: 12px;
    margin-right: 0.5em;
    background: url(/images/border_red.png) no-repeat;
    background-size: 100% 100%;
}
.my-article-time{
	font-size: 0.6rem;
    color: #888;
    display: inline-block;
}
.btn{
	background-color:#f23030 ;
	width: 100%;
    height: .45rem;
    line-height: .46rem;
    background-color: #eee;
    border-radius: 2px;
    margin-top: .25rem;
    border: 0;
}
.loading{
    height: 1.5rem;
    text-align: center;
    padding-top: 0.2rem;	
}
.loading em{
	background:url(/images/loading.gif) center 2px no-repeat;
	display: inline-block;
    width: 21px;
    height: 23px;
    background-size: 21px 21px;
    vertical-align: top;
}
.loading>span{
	color: #b3b3b3;
    font-size: .75rem;
    display: inline-block;
    padding-top:.1rem;
    line-height: 0.8rem;
    margin-left: 10px;
}
